[user]
	name = David Larson
	email = dplarson@ucsd.edu 
[github]
	user = dplarson
[core]
	editor = mvim -f
	excludesfile = ~/.gitignore
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    f  = fetch -v
    s  = summary
    i  = info
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
[push]
    default = current
